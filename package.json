{
  "name": "objectstrees",
  "version": "1.0.0",
  "description": "mangage tree of objects",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t objectstrees .",
    "docker:run": "docker run -p 3000:3000 -d objectstrees",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@jacquesparis/objects-model": "file:../objects-model",
    "@loopback/boot": "^2.5.1",
    "@loopback/context": "^3.12.0",
    "@loopback/core": "^2.11.0",
    "@loopback/openapi-v3": "^3.4.9",
    "@loopback/repository": "^2.11.2",
    "@loopback/rest": "^3.3.2",
    "@loopback/rest-explorer": "^2.2.10",
    "@loopback/service-proxy": "^2.3.8",
    "@types/lodash": "^4.14.162",
    "@types/uuid": "^7.0.4",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "loopback-connector-mongodb": "^4.2.0",
    "multer": "^1.4.2",
    "p-event": "^4.2.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@loopback/build": "^4.0.0",
    "@loopback/eslint-config": "^6.0.6",
    "@loopback/testlab": "^2.0.0",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/multer": "^1.4.4",
    "@types/node": "^10.17.42",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^6.3.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.8.3"
  }
}
