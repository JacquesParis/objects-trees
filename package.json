{
  "name": "@jacquesparis/objects-trees",
  "version": "0.0.1-SNAPSHOT.17",
  "description": "manage tree of objects",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t objectstrees .",
    "docker:run": "docker run -p 3000:3000 -d objectstrees",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run clean && npm run build",
    "start": "node main.js",
    "dev": "npm uninstall @jacquesparis/objects-model @jacquesparis/objects-angular @jacquesparis/objects-website && npm install --save ../objects-model ../objects-angular/dist ../objects-website ",
    "update": "npm uninstall @jacquesparis/objects-model @jacquesparis/objects-angular @jacquesparis/objects-website  && npm install --save @jacquesparis/objects-model @jacquesparis/objects-angular @jacquesparis/objects-website ",
    "lib": "npm update && npm run build && git add -A && git commit -m \"lib\" && git push origin HEAD && npm publish --access=public"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@jacquesparis/objects-angular": "file:../objects-angular/dist",
    "@jacquesparis/objects-model": "file:../objects-model",
    "@jacquesparis/objects-website": "file:../objects-website",
    "@loopback/authentication": "^7.0.6",
    "@loopback/authentication-jwt": "^0.7.6",
    "@loopback/authorization": "^0.7.6",
    "@loopback/boot": "^3.2.0",
    "@loopback/build": "^6.2.9",
    "@loopback/context": "^3.14.0",
    "@loopback/core": "^2.14.0",
    "@loopback/eslint-config": "^10.0.5",
    "@loopback/openapi-v3": "^5.1.3",
    "@loopback/repository": "^3.4.0",
    "@loopback/rest": "^8.0.0",
    "@loopback/rest-explorer": "^3.0.6",
    "@loopback/service-proxy": "^3.0.6",
    "@loopback/testlab": "^3.2.11",
    "bootstrap": "^4.6.0",
    "css": "^3.0.0",
    "express": "^4.17.1",
    "jquery": "^3.3.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "loopback-connector-mongodb": "^4.2.0",
    "loopback-connector-rest": "^4.0.1",
    "moment": "^2.29.1",
    "mustache": "^4.1.0",
    "p-event": "^4.2.0",
    "sharp": "^0.27.1"
  },
  "devDependencies": {
    "@types/css": "0.0.31",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.18",
    "@types/html": "^1.0.0",
    "@types/lodash": "^4.14.168",
    "@types/mustache": "^4.1.1",
    "@types/node": "^10.17.51",
    "@types/sharp": "^0.26.1",
    "@types/uuid": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-mocha": "^8.0.0",
    "source-map-support": "^0.5.19",
    "tslib": "^2.1.0",
    "typescript": "~4.0.3"
  }
}
